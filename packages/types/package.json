{
  "name": "@workspace/types",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "description": "Types TypeScript partag√©s pour l'application Ona UI",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint . --max-warnings 0",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@workspace/eslint-config": "*",
    "@workspace/typescript-config": "*",
    "typescript": "^5.9.2"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./auth": {
      "types": "./dist/auth.d.ts",
      "import": "./dist/auth.js"
    },
    "./components": {
      "types": "./dist/components.d.ts",
      "import": "./dist/components.js"
    },
    "./categories": {
      "types": "./dist/categories.d.ts",
      "import": "./dist/categories.js"
    },
    "./api": {
      "types": "./dist/api.d.ts",
      "import": "./dist/api.js"
    },
    "./common": {
      "types": "./dist/common.d.ts",
      "import": "./dist/common.js"
    },
    "./assets": {
      "types": "./dist/assets.d.ts",
      "import": "./dist/assets.js"
    },
    "./http_context": {
      "types": "./dist/http_context.d.ts",
      "import": "./dist/http_context.js"
    },
    "./stripe_types": {
      "types": "./dist/stripe_types.d.ts",
      "import": "./dist/stripe_types.js"
    }
  }
}